{
  "name": "@sasjs/angular-seed-app",
  "repository": "https://github.com/sasjs/angular-seed-app",
  "description": "Angular Seed App for SASjs",
  "version": "2.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production --aot && cp -R docs dist/",
    "build-dev": "ng build",
    "test": "ng test",
    "lint:fix": "npx prettier --write \"src/**/*.{ts,tsx,js,jsx,html,css,sass,less,json,yml,md,graphql}\"",
    "lint": "npx prettier --check \"src/**/*.{ts,tsx,js,jsx,html,css,sass,less,json,yml,md,graphql}\"",
    "e2e": "ng e2e",
    "build-watch": "ng build --watch",
    "deploy": "rsync -avhe ssh ./dist/* --delete $SSH_ACCOUNT:$DEPLOY_PATH",
    "sync": "./node_modules/.bin/watch --wait=3 \"echo Account: $SSH_ACCOUNT && npm run deploy && echo 'App is synced!'\" dist"
  },
  "private": true,
  "keywords": [
    "sas",
    "SASjs",
    "viya",
    "sasjs_seed_app"
  ],
  "dependencies": {
    "@angular/animations": "~13.2.6",
    "@angular/cdk": "^12.2.8",
    "@angular/common": "~13.2.6",
    "@angular/compiler": "~13.2.6",
    "@angular/core": "~13.2.6",
    "@angular/forms": "~13.2.6",
    "@angular/platform-browser": "~13.2.6",
    "@angular/platform-browser-dynamic": "~13.2.6",
    "@angular/router": "~13.2.6",
    "@clr/angular": "^4.0.16",
    "@clr/core": "^4.0.15",
    "@clr/icons": "5.0.3",
    "@clr/ui": "5.0.3",
    "@sasjs/adapter": "^3.7.14",
    "@sasjs/core": "^4.17.1",
    "@webcomponents/webcomponentsjs": "^2.6.0",
    "moment": "^2.29.1",
    "rxjs": "~6.6.7",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.2.6",
    "@angular/cli": "~13.2.6",
    "@angular/compiler-cli": "~13.2.6",
    "@angular/language-service": "~13.2.6",
    "@types/jasmine": "^3.9.1",
    "@types/jasminewd2": "^2.0.10",
    "@types/node": "^16.11.6",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.9.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "prettier": "^2.5.1",
    "protractor": "~7.0.0",
    "ts-node": "~10.2.1",
    "typescript": "~4.5.5",
    "watch": "^0.13.0"
  }
}
