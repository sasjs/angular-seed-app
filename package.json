{
  "name": "@sasjs/angular-seed-app",
  "repository": "https://github.com/sasjs/angular-seed-app",
  "description": "Angular Seed App for SASjs",
  "version": "2.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 3000",
    "build": "ng build --configuration production --aot && cp -R docs dist/",
    "build-dev": "ng build",
    "test": "ng test",
    "lint:fix": "npx prettier --write \"src/**/*.{ts,tsx,js,jsx,html,css,sass,less,json,yml,md,graphql}\"",
    "lint": "npx prettier --check \"src/**/*.{ts,tsx,js,jsx,html,css,sass,less,json,yml,md,graphql}\"",
    "e2e": "ng e2e",
    "build-watch": "ng build --watch",
    "deploy": "rsync -avhe ssh ./dist/* --delete $SSH_ACCOUNT:$DEPLOY_PATH",
    "sync": "./node_modules/.bin/watch --wait=3 \"echo Account: $SSH_ACCOUNT && npm run deploy && echo 'App is synced!'\" dist"
  },
  "private": true,
  "keywords": [
    "sas",
    "SASjs",
    "viya",
    "sasjs_seed_app"
  ],
  "dependencies": {
    "@angular/animations": "^15.2.9",
    "@angular/cdk": "^15.2.9",
    "@angular/common": "^15.2.9",
    "@angular/compiler": "^15.2.9",
    "@angular/core": "^15.2.9",
    "@angular/forms": "^15.2.9",
    "@angular/platform-browser": "^15.2.9",
    "@angular/platform-browser-dynamic": "^15.2.9",
    "@angular/router": "^15.2.9",
    "@sasjs/adapter": "^3.10.14",
    "@sasjs/cli": "3.17.0",
    "@sasjs/core": "^4.34.1",
    "@webcomponents/webcomponentsjs": "^2.6.0",
    "moment": "^2.29.3",
    "@sasjs/ngx-sasjs": "^1.0.4",
    "rxjs": "~7.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.8",
    "@angular/cli": "^15.2.8",
    "@angular/compiler-cli": "^15.2.9",
    "@angular/language-service": "^15.2.9",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.4",
    "@types/jasmine": "^4.0.3",
    "@types/jasminewd2": "^2.0.10",
    "@types/node": "^20.1.2",
    "codelyzer": "^6.0.2",
    "jasmine-core": "~4.2.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "prettier": "^2.5.1",
    "protractor": "~7.0.0",
    "ts-node": "~10.8.1",
    "typescript": "~4.9.5",
    "watch": "^0.13.0"
  }
}