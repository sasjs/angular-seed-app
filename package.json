{
  "name": "@sasjs/angular-seed-app",
  "repository": "https://github.com/sasjs/angular-seed-app",
  "description": "Angular Seed App for SASjs",
  "version": "2.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production --aot && cp -R docs dist/",
    "build-dev": "ng build",
    "test": "ng test",
    "lint:fix": "npx prettier --write \"src/**/*.{ts,tsx,js,jsx,html,css,sass,less,json,yml,md,graphql}\"",
    "lint": "npx prettier --check \"src/**/*.{ts,tsx,js,jsx,html,css,sass,less,json,yml,md,graphql}\"",
    "e2e": "ng e2e",
    "build-watch": "ng build --watch",
    "deploy": "rsync -avhe ssh ./dist/* --delete $SSH_ACCOUNT:$DEPLOY_PATH",
    "sync": "./node_modules/.bin/watch --wait=3 \"echo Account: $SSH_ACCOUNT && npm run deploy && echo 'App is synced!'\" dist"
  },
  "private": true,
  "keywords": [
    "sas",
    "SASjs",
    "viya",
    "sasjs_seed_app"
  ],
  "dependencies": {
    "@angular/animations": "^14.1.0",
    "@angular/cdk": "^13.3.8",
    "@angular/common": "^14.1.0",
    "@angular/compiler": "^14.1.0",
    "@angular/core": "^14.1.0",
    "@angular/forms": "^14.1.0",
    "@angular/platform-browser": "^14.1.0",
    "@angular/platform-browser-dynamic": "^14.1.0",
    "@angular/router": "^14.1.0",
    "@clr/angular": "^13.5.0",
    "@cds/core": "^6.1.0",
    "@clr/icons": "13.0.2",
    "@clr/ui": "13.5.0",
    "@sasjs/adapter": "^3.10.2",
    "@sasjs/cli": "^3.13.6",
    "@sasjs/core": "^4.27.14",
    "@webcomponents/webcomponentsjs": "^2.6.0",
    "moment": "^2.29.3",
    "rxjs": "~7.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.1.0",
    "@angular/cli": "^14.1.0",
    "@angular/compiler-cli": "^14.1.0",
    "@angular/language-service": "^14.1.0",
    "@types/jasmine": "^4.0.3",
    "@types/jasminewd2": "^2.0.10",
    "@types/node": "^18.0.0",
    "codelyzer": "^6.0.2",
    "jasmine-core": "~4.2.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "prettier": "^2.5.1",
    "protractor": "~7.0.0",
    "ts-node": "~10.8.1",
    "typescript": "~4.7.4",
    "watch": "^0.13.0"
  }
}